{
  "name": "node-ts-starter",
  "version": "0.0.1",
  "private": true,
  "description": "Boilerplate for node.ts project",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --project tsconfig.json --watch",
    "start": "npm run build && node ./dist",
    "start:dev": "nodemon",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "Andriy Horen",
  "license": "MIT",
  "dependencies": {
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@swc/core": "^1.4.11",
    "@swc/helpers": "^0.5.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^7.4.0"
  }
}
